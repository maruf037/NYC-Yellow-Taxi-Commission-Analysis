	partition DimDate = calculated
		mode: import
		source =
				ADDCOLUMNS(
				    CALENDAR(
				        DATE(2025, 09, 01),
				        DATE(2025, 09, 30)
				    ),
				    "Year", YEAR ( [Date] ),
				    "Month Number", MONTH ( [Date] ),
				    "Month Name", FORMAT ( [Date], "MMM" ),
				    "Year-Month", FORMAT ( [Date], "YYYY-MM" ),
				    "Day", DAY ( [Date] ),
				    "Weekday", FORMAT ( [Date], "DDD" ),
				    "IsWeekend", IF(
				        WEEKDAY([Date], 2) > 5, TRUE(),
				        FALSE()
				    )
				)

	annotation PBI_Id = 09c4c73a3d8c4649b09e98e65314dbab

